<soto:app xmlns:soto="sapia:soto"
          xmlns:stm="sapia:stm"
          xmlns:cocoon="sapia:cocoon"
          xmlns:example="sapia:example">
          
  <soto:namespace prefix="example">
    <def name="types" class="org.sapia.soto.examples.TypeService" />
  </soto:namespace>
  
  <soto:service id="typeService">  
    <example:types>
      <someShort>
        <soto:short value="1" />
      </someShort>    
      <someInt>
        <soto:int value="2" />
      </someInt>
      <someLong>
        <soto:long value="3" />
      </someLong>      
      <someFloat>
        <soto:float value="4" />
      </someFloat>      
      <someDouble>
        <soto:double value="5" />
      </someDouble>            
      <someBoolean>
        <soto:boolean value="true" />
      </someBoolean>            
      <someDate>
        <soto:date value="2000/12/25" pattern="yyyy/MM/DD" />
      </someDate>                  
    </example:types>
  </soto:service>
</soto:app>
