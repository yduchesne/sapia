<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <name>Soto LDAP Integration</name>
  <description>Integrates LDAP through the LDAP SDK</description>

  <url>http://www.sapia-oss.org/projects/soto/subprojects/ldap/index.html</url>
  <artifactId>sapia_soto_ldap</artifactId>

  <parent>
    <groupId>org.sapia</groupId>
    <artifactId>sapia_soto_parent</artifactId>
    <version>3.2-SNAPSHOT</version>
  </parent>

  <dependencies>

    <dependency>
      <groupId>org.sapia</groupId>
      <artifactId>sapia_soto</artifactId>
      <version>${project.version}</version>
    </dependency>
    <dependency>
      <groupId>org.apache.directory.server</groupId>
      <artifactId>apacheds-all</artifactId>
      <version>1.5.5</version>
      <optional>true</optional>
    </dependency>
    <dependency>
        <groupId>org.slf4j</groupId>
        <artifactId>slf4j-api</artifactId>
        <version>1.4.3</version>
        <optional>true</optional>
    </dependency>
    <dependency>
        <groupId>org.slf4j</groupId>
        <artifactId>slf4j-log4j12</artifactId>
        <version>1.4.3</version>
        <optional>true</optional>
    </dependency>
    <dependency>
        <groupId>ldapsdk</groupId>
        <artifactId>ldapsdk</artifactId>
        <version>4.1</version>
    </dependency>
  </dependencies>

</project>
