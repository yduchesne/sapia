<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean id="regis" class="org.sapia.regis.spring.RegisAnnotationProcessor">
    <property name="bootstrap" value="bootstrap.properties" />
  </bean>
  
  <bean id="lookup" class="org.sapia.regis.sample.spring.LookupConfigHolder" />
  <bean id="properties" class="org.sapia.regis.sample.spring.PropertyConfigHolder" />
  

</beans>