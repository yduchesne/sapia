<magnet xmlns="http://schemas.sapia-oss.org/magnet/core/"
        name="AOP Example" description="Very basic AOP sample">
        
  <launcher type="java" name="AOP" 
            mainClass="org.sapia.soto.SotoMain"
            args="${soto.home}/etc/aopSample.xml">

      <profile name="default">
          <classpath>
              <path directory="${soto.home}/lib">
                 <include pattern="**/*.jar" />
              </path>
              <path directory="${soto.home}/dist">
                <include pattern="*.jar" />
              </path>
        </classpath>        
      </profile>
  </launcher>
</magnet>      
