<magnet xmlns="http://schemas.sapia-oss.org/magnet/core/"
        name="RestartApplication" description="Restart app that restart itself using Corus Interop">


  <launcher type="java" name="restartApp" mainClass="org.sapia.corus.examples.RestartApplication"
            default="test" isDaemon="false" waitTime="2000">
      <profile name="test">
          <classpath>
					    <path directory="lib">
					      <include pattern="*.jar" />
					    </path>
          </classpath>
      </profile>
  </launcher>
</magnet>        
